[
    {
        "environmentFiles": [
            {
                "type": "s3",
                "value": "<envfile>"
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "sovocal-production-tdf",
                "awslogs-region": "us-east-2",
                "awslogs-stream-prefix": "ecs"
            }
        },
        "entryPoint": [
            "/app/bin/sovocal-production-entrypoint"
        ],
        "portMappings": [
            {
              "hostPort": 80,
              "protocol": "tcp",
              "containerPort": 80
            }
        ],
        "command": [
            "/usr/bin/supervisord",
            "-c",
            "/etc/supervisord.conf"
        ],
        "cpu": <cpu>,
        "memory": <memory>,
        "memoryReservation": <memory-reservation>,
        "workingDirectory": "/app",
        "image": "<IMG>",
        "essential": true,
        "name": "<container-name>"
    }
]