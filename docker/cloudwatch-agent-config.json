{
    "agent": {
            "metrics_collection_interval": 60,
            "region": "us-east-2",
            "run_as_user": "root",
            "credentials_path":"/app/.aws/credentials"
    },
    "logs": {
            "logs_collected": {
                    "files": {
                            "collect_list": [
                                    {
                                         "file_path": "/app/log/production.log",
                                         "log_group_name": "sovocal-testing-logs",
                                         "log_stream_name": "production-log",
                                         "timestamp_format" :"%b %d %H:%M:%S"
                                    },
                                    {
                                        "file_path": "/var/log/nginx/access.log",
                                        "log_group_name": "sovocal-testing-logs",
                                        "log_stream_name": "nginx-access-log",
                                        "timestamp_format" :"%b %d %H:%M:%S"
                                    },
                                    {
                                        "file_path": "/var/log/nginx/error.log",
                                        "log_group_name": "sovocal-testing-logs",
                                        "log_stream_name": "nginx-error-log",
                                        "timestamp_format" :"%b %d %H:%M:%S"
                                    }
                            ]
                    }
           }
   }
}