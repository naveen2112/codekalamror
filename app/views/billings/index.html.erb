      <div>
        <%= form_for current_company, url: cancel_subscription_billings_path, html: { id: "update-subscription" }, method: :put do |form| %>
          <% if current_company.active? %>
            <%= form.hidden_field :subscription_status, value: "cancelled" %>
            <%= form.hidden_field :subscription_cancelled_at, value: Date.current %>
            <%= form.submit "Cancel subscription", class: "btn btn-danger" %>
          <% else %>
            <%= form.hidden_field :subscription_status, value: "active" %>
            <%= form.hidden_field :subscription_cancelled_at, value:  "" %>
            <%= form.submit "Renew subscription", class: "btn btn-primary" %>
          <% end %>
        <% end %>
