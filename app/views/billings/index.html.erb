<div class="profile-wrap">
  <div class="title-wrap">
    <h6 class="title">Billing Settings</h6>
    <p class="text">Edit any of your details by changing the relevant field below.</p>
  </div>
  <div class="billing-wrap">
    <div class="user-available-wrap">
      <% if current_company.active? %>
        <p class="second-lev-title">Next billing date</p>
        <p class="count"><%= current_company.next_billing_date&.strftime("%d-%m-%Y") %></p>
      <% else %>
        <p class="second-lev-title">Your validity of the Subscription will expire in</p>
        <p class="count"><%=  current_company.subscription_days_left %> days</p>
      <% end %>
      <p class="second-lev-title mt-3">Number of users available</p>
      <p class="count"><%= current_company.users.count %></p>
      <% if current_company.billable? %>
      <p class="second-lev-title mt-3">Monthly subscription total</p>
      <p class="count">$ <%= current_company.charged_amount %></p>
      <% end %>
      <% if current_company.user_limit_warning == current_company.users.count && current_company.sales_led? %>
        <p class="error-msg d-flex align-items-center">
