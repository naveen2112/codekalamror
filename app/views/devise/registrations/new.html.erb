                      <p class="text-center login-link">Do you already have an account?
                        <a class="text-link" href="/users/sign_in">Log in</a></p>
                    </div>

                  </div>

                  <div id="page-2">

                    <%= f.fields_for "cards_attributes[0]" do |form| %>
