<section class="auth">
  <div class="container-fluid">
    <div class="row auth-wrap">
      <div class="col-md-12 col-lg-6 col-sm-12 position-relative left-column">

        <div class="authentication-container signUpFormAr">
          <div class="signInFormWrap">
            <div class="container-wrap">

              <h2 class="formTitle">Letâ€™s Get Started</h2>
              <p class="formText">Enter your details below to sign up for an account.</p>

              <div class="progress">
                <div class="progress-bar" role="progressbar" style="width:50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
              </div>

              <div class="formWrap">

                <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {id: "sign-up", class: "needs-validation"}) do |f| %>

                  <div id="page-1">

                    <%= f.fields_for "company_attributes" do |form| %>
                      <div class="form-group required">
                        <label class="control-label" for="InputOrganisationName">Organisation name</label>
                        <%= form.text_field :name, class: "form-control", placeholder: "Organisation name" %>
                      </div>

                      <%= form.hidden_field :trail_start_date, value: Date.current %>
                      <%= form.hidden_field :plan_type, value: "product_led" %>
                      <%= form.hidden_field :billing_type, value: "monthly", :id=>"billing_type"%>

                      <div class="form-group required">
                        <label class="control-label" for="InputOrganisationName">Organisation URL</label>
                        <%= form.url_field :url, class: "form-control", placeholder: "Organisation url" %>
                      </div>
                    <% end %>

                    <div class="form-group required">
                      <label class="control-label" for="InputFirstName">First name</label>
                      <%= f.text_field :first_name, class: "form-control", placeholder: "First name" %>
                    </div>

                    <div class="form-group">
                      <label class="control-label" for="InputLastName">Last name</label>
                      <%= f.text_field :last_name, class: "form-control", placeholder: "Last name" %>
                    </div>

                    <div class="form-group required">
                      <label class="control-label" for="InputBusinessName">Business email</label>
                      <%= f.email_field :email, class: "form-control", placeholder: "Business email" %>
                    </div>


                    <div class="form-group required position-relative">
                      <label class="control-label" for="InputPassword">Password</label>
                      <%= f.password_field :password, class: "form-control password-field", placeholder: "Password" %>
                      <button class="btn user-password-button" id="user-password-button" type="button">
