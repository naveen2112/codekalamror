            function GetDates(startDate, daysToAdd) {
                var aryDates = [];

                for (var i = 0; i <= daysToAdd; i++) {
                    var currentDate = new Date();
                    currentDate.setDate(startDate.getDate() + i);
                    aryDates.push(DayAsString(currentDate.getDay()));
                }

                return aryDates;
